} number-ar_3;

# make the lam-alef ligature have colored hamzas if wanted
# not used??
lookup splitAlefHamza {
    sub alefHamzaabove-ar by alef-ar hamzaabove-ar;
    sub alefHamzabelow-ar by alef-ar hamzabelow-ar;
} splitAlefHamza;

# make alef into alef.wide
lookup alef2wide {
