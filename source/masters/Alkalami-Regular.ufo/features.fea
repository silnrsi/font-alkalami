        sub waw-ar by waw-ar.alt;
        sub waw-ar.fina by waw-ar.fina.alt;
        sub yehbarree-ar.fina by yehbarree-ar.fina.alt;
        # alternate tail - not used
        sub ainThreedots-ar.fina.ss05 by ainThreedots-ar.fina.alt.ss05;
        # small latin and arabic digits for end of ayah - handled by calt
        sub @cno_numAlt by @c_numAlt;
        sub @cno_numArAlt by @c_numArAlt;
    } salt_lkp;

    # multiple tail swashes - handled by calt
    lookup salt_lkp2 {
        sub alefMaksura-ar.fina from [alefMaksura-ar.fina.shrtail alefMaksura-ar.fina.longtail];
        sub alefMaksura-ar from [alefMaksura-ar.shrtail alefMaksura-ar.longtail];
        sub noon-ar.fina from [noon-ar.fina.shrtail noon-ar.fina.longtail];
        sub noon-ar from [noon-ar.shrtail noon-ar.longtail];
        sub noonAfrican-ar.fina from [noonAfrican-ar.fina.shrtail noonAfrican-ar.fina.longtail];
        sub noonAfrican-ar from [noonAfrican-ar.shrtail noonAfrican-ar.longtail];
        sub noonTwodotsbelow-ar.fina from [noonTwodotsbelow-ar.fina.shrtail noonTwodotsbelow-ar.fina.longtail];
        sub noonTwodotsbelow-ar from [noonTwodotsbelow-ar.shrtail noonTwodotsbelow-ar.longtail];
        sub noonghunna-ar.fina from [noonghunna-ar.fina.shrtail noonghunna-ar.fina.longtail];
        sub noonghunna-ar from [noonghunna-ar.shrtail noonghunna-ar.longtail];
        sub seen-ar.fina from [seen-ar.fina.shrtail seen-ar.fina.longtail];
        sub seen-ar from [seen-ar.shrtail seen-ar.longtail];
        sub sheen-ar.fina from [sheen-ar.fina.shrtail sheen-ar.fina.longtail];
        sub sheen-ar from [sheen-ar.shrtail sheen-ar.longtail];
        sub yeh-ar.fina from [yeh-ar.fina.shrtail yeh-ar.fina.longtail];
        sub yeh-ar from [yeh-ar.shrtail yeh-ar.longtail];
        sub noon-ar.ss01 from [noon-ar.shrtail.ss01 noon-ar.longtail.ss01];
        sub noon-ar.fina.ss01 from [noon-ar.shrtail.ss01 noon-ar.longtail.ss01];
        sub yeh-ar.ss01 from [yeh-ar.shrtail.ss01 yeh-ar.longtail.ss01];
        sub yeh-ar.fina.ss01 from [yeh-ar.fina.shrtail.ss01 yeh-ar.fina.longtail.ss01];
    } salt_lkp2;

} salt;

feature calt {
    # No stacking, rule 1:
    # Use the alternate form for chars that do not stack above hah medials.
    lookup calt_lkp1 {
        lookupflag RightToLeft IgnoreMarks;
        sub @pre_hah_non_alt' @hahMedials by @pre_hah_alt;
        # Slightly shorter tail for yeh-barree before beh/yeh/noon forms:
        sub @pre_yb_need_alt yehbarree-ar.fina' by yehbarree-ar.fina.alt;
    } calt_lkp1;

    # No stacking, rule 2:
    # Use the alternate form for chars that do not stack above hah finals.
    lookup prehah_alt {
        lookupflag RightToLeft IgnoreMarks;
        sub @pre_hah_non_alt' @hahFinals by @pre_hah_alt;
    } prehah_alt;

    # No stacking, rule 3:
    lookup hahfinal_alt {
        lookupflag RightToLeft IgnoreMarks;
        sub @pre_hah_alt @hahFinals' by @hahFinals_alt;
    } hahfinal_alt;

    lookup calt_lkp2 {
        lookupflag RightToLeft IgnoreMarks;
        # subs to avoid collisions in strings with dal-ar
        sub seen-ar space-ar dal-ar' by dal-ar.alt;
        sub seen-ar space-ar thal-ar' by thal-ar.alt;
        # subs to avoid collisions in strings with yeh-ar
        sub yeh-ar' comma-ar space-ar waw-ar by yeh-ar.longtail;
        sub yeh-ar.fina' space-ar lam-ar.init reh-ar.fina by yeh-ar.fina.shrtail;
        sub yeh-ar.fina' space-ar ghain-ar by yeh-ar.fina.shrtail;
        sub yeh-ar.fina' space-ar reh-ar by yeh-ar.fina.longtail;
        sub yeh-ar.fina' space-ar waw-ar by yeh-ar.fina.longtail;
        # subs to avoid collisions in strings with noon-ar
        sub noon-ar' space-ar seen-ar by noon-ar.longtail;
        sub noon-ar' space-ar teh-ar.init waw-ar.fina by noon-ar.longtail;
        sub noon-ar' space-ar teh-ar.init noon-ar.fina by noon-ar.longtail;
        sub noon-ar' space-ar theh-ar.init noon-ar.fina by noon-ar.longtail;
        sub noon-ar' space-ar dal-ar waw-ar by noon-ar.longtail;
        sub noon-ar' space-ar beh-ar.init by noon-ar.longtail;
        sub noon-ar' space-ar beh-ar by noon-ar.longtail;
        sub noon-ar' space-ar noon-ar.init noon-ar.fina by noon-ar.longtail;
        sub noon-ar.fina' space-ar beh-ar.init by noon-ar.fina.longtail;
        sub noon-ar.fina' space-ar beh-ar by noon-ar.fina.longtail;
        sub noon-ar.fina' space-ar seen-ar.init waw-ar.fina by noon-ar.fina.shrtail;
        sub noon-ar.fina' space-ar seen-ar.init noon-ar.fina by noon-ar.fina.shrtail;
        sub noon-ar.fina' space-ar reh-ar by noon-ar.fina.longtail;
        sub noon-ar.fina' space-ar teh-ar.init waw-ar.fina by noon-ar.fina.longtail;
        sub noon-ar.fina' space-ar dal-ar waw-ar by noon-ar.fina.longtail;
        # subs to avoid collisions in strings with noonAfrican-ar
        sub noonAfrican-ar' space-ar seen-ar by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar teh-ar.init waw-ar.fina by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar teh-ar.init noon-ar.fina by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar theh-ar.init noon-ar.fina by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar dal-ar waw-ar by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar beh-ar.init by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar beh-ar by noonAfrican-ar.longtail;
        sub noonAfrican-ar' space-ar noonAfrican-ar.init noonAfrican-ar.fina by noonAfrican-ar.longtail;
        sub noonAfrican-ar.fina' space-ar beh-ar.init by noonAfrican-ar.fina.longtail;
        sub noonAfrican-ar.fina' space-ar beh-ar by noonAfrican-ar.fina.longtail;
        sub noonAfrican-ar.fina' space-ar seen-ar.init waw-ar.fina by noonAfrican-ar.fina.shrtail;
        sub noonAfrican-ar.fina' space-ar seen-ar.init noon-ar.fina by noonAfrican-ar.fina.shrtail;
        sub noonAfrican-ar.fina' space-ar reh-ar by noonAfrican-ar.fina.longtail;
        sub noonAfrican-ar.fina' space-ar teh-ar.init waw-ar.fina by noonAfrican-ar.fina.longtail;
        sub noonAfrican-ar.fina' space-ar dal-ar waw-ar by noonAfrican-ar.fina.longtail;
        # subs to avoid collisions in strings with seen-ar
        sub seen-ar' space-ar seen-ar.init waw-ar.fina by seen-ar.shrtail;
        sub seen-ar' space-ar beh-ar.init waw-ar.fina by seen-ar.longtail;
        sub seen-ar' space-ar beh-ar.init by seen-ar.longtail;
        sub seen-ar.fina' space-ar beh-ar.init waw-ar.fina by seen-ar.fina.longtail;
        sub seen-ar.fina' space-ar alefHamzabelow-ar reh-ar by seen-ar.fina.longtail;
        sub seen-ar.fina' space-ar alefHamzaabove-ar waw-ar by seen-ar.fina.longtail;
        sub seen-ar.fina' space-ar ghain-ar by seen-ar.fina.longtail;
        # subs to avoid collisions in strings with waw-ar
        sub noon-ar.fina space-ar waw-ar' by waw-ar.alt;
        sub noon-ar space-ar waw-ar' by waw-ar.alt;
        # subs to avoid collisions in strings with waw-ar and noonAfrican-ar
        sub noonAfrican-ar.fina space-ar waw-ar' by waw-ar.alt;
        sub noonAfrican-ar space-ar waw-ar' by waw-ar.alt;
        #subs to get a short, high lam-ar.init with jeem/hah/khah
        sub lam-ar.init' jeem-ar.medi by lam-ar.init.high;
        sub lam-ar.init' hah-ar.medi by lam-ar.init.high;
        sub lam-ar.init' khah-ar.medi by lam-ar.init.high;
        sub lam-ar.init' nyeh-ar.medi by lam-ar.init.high;
        #subs to get a short, high alef-ar.init with lam followed by jeem/hah/khah
        sub alef-ar' lam-ar.init jeem-ar.medi by alef-ar.high;
        sub alef-ar' lam-ar.init hah-ar.medi by alef-ar.high;
        sub alef-ar' lam-ar.init khah-ar.medi by alef-ar.high;
        sub alef-ar' lam-ar.init nyeh-ar.medi by alef-ar.high;
        # replace sequence of digits, up to three places
        # Uniscribe or Harfbuzz 'latn'
        sub @EndofAyah' lookup Ayah_alt3 @Digits' lookup digit_small @Digits' lookup digit_small @Digits' lookup digit_small;
        sub @EndofAyah' lookup Ayah_alt2 @Digits' lookup digit_small @Digits' lookup digit_small;
        sub @EndofAyah @Digits' lookup digit_small;
        # HafBuzz 'arab'
        sub @Digits' lookup digit_small @Digits' lookup digit_small @Digits' lookup digit_small @EndofAyah' lookup Ayah_alt3;
        sub @Digits' lookup digit_small @Digits' lookup digit_small @EndofAyah' lookup Ayah_alt2;
        sub @Digits' lookup digit_small @EndofAyah;
    } calt_lkp2;

    # to keep Uniscribe from breaking/crashing
    lookup UniscribeFix {
        sub @EndofAyah' @Digits.small @Digits.small @Digits.small by @EndofAyah3;
        sub @EndofAyah' @Digits.small @Digits.small by @EndofAyah2;
    } UniscribeFix;

} calt;

